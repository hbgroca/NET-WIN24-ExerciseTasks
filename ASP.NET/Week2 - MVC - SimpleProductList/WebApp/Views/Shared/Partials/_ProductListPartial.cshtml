@model IEnumerable<Product>

<div class="products">
    @if (Model.Count() == 0)
    {
        <p>Det finns inga produkter tillgängliga.</p>
    }
    else
    {
        foreach (var product in Model)
        {
            <div class="product">
                 @await Html.PartialAsync("Partials/_ProductPartial", product)
            </div>
        }
    }
</div>